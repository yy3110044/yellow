<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yy.yellow.mapper.AdminUserMapper">
	<insert id="add" parameterType="com.yy.yellow.po.AdminUser">
		<selectKey keyProperty="id" order="AFTER" resultType="int">
			SELECT LAST_INSERT_ID()
		</selectKey>
		insert into yellow_admin_user(id, userName, passWord, createTime)
		values(#{id}, #{userName}, #{passWord}, #{createTime})
	</insert>
	
	<delete id="delete" parameterType="int">
		delete from yellow_admin_user where id = #{id}
	</delete>
</mapper>